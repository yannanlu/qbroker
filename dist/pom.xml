<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <name>dist</name>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>dist</artifactId>
  <packaging>pom</packaging>
  <parent>
    <groupId>org.qbroker</groupId>
    <artifactId>qbroker</artifactId>
    <version>1.2.6</version>
  </parent>
  <dependencies>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>common</artifactId>
      <version>1.2.10</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>net</artifactId>
      <version>1.1.9</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>event</artifactId>
      <version>1.1.6</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>json</artifactId>
      <version>1.2.1</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>monitor</artifactId>
      <version>1.1.22</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>aws</artifactId>
      <version>1.0.2</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>jms</artifactId>
      <version>1.2.16</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>jndi</artifactId>
      <version>1.1.1</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>jaas</artifactId>
      <version>1.1.1</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>receiver</artifactId>
      <version>1.1.4</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>persister</artifactId>
      <version>1.1.9</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>node</artifactId>
      <version>1.1.12</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>cluster</artifactId>
      <version>1.1.0</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>flow</artifactId>
      <version>1.2.29</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>wmq</artifactId>
      <version>1.1.5</version>
    </dependency>
    <dependency>
      <groupId>org.qbroker</groupId>
      <artifactId>sonicmq</artifactId>
      <version>1.1.7</version>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-assembly-plugin</artifactId>
        <version>2.6</version>
        <configuration>
          <descriptors>
            <descriptor>src/assembly/bin.xml</descriptor>
          </descriptors>
          <finalName>qbroker-${project.version}</finalName>
          <appendAssemblyId>false</appendAssemblyId>
        </configuration>
        <executions>
          <execution>
            <id>make-assembly</id>
            <phase>package</phase>
            <goals>
              <goal>single</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
</project>
